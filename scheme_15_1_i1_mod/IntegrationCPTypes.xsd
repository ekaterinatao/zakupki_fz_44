				<xs:complexType>
					<xs:sequence>
						<xs:element name="indicatorInfo" type="dlIndicatorInfoType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Показатель критерия</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
